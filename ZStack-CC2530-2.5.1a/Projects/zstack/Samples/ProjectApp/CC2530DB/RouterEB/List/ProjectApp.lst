    470            if ( osal_queue_receive( OsalQueue, &inMsg, 0 ) == pdPASS )
    471            {
    472              uint8 cmndId = inMsg.cmnd;
    473              uint32 counter = osal_build_uint32( inMsg.cbuf, 4 );
    474          
    475              switch ( cmndId )
    476              {
    477                case CMD_INCR:
    478                  counter += 1;  /* Increment the incoming counter */
    479                                 /* Intentionally fall through next case */
    480          
    481                case CMD_ECHO:
    482                {
    483                  userQueue_t outMsg;
    484          
    485                  outMsg.resp = RSP_CODE | cmndId;  /* Response ID */
    486                  osal_buffer_uint32( outMsg.rbuf, counter );    /* Increment counter */
    487                  osal_queue_send( UserQueue1, &outMsg, 0 );  /* Send back to UserTask */
    488                  break;
    489                }
    490                
    491                default:
    492                  break;  /* Ignore unknown command */    
    493              }
    494            }
    495          }
